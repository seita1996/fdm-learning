{
  "name": "fdm-learning",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "slidev decks/fdm-intro/slides.md --open",
    "dev:fdm": "slidev decks/fdm-intro/slides.md --open",
    "dev:neverthrow": "slidev decks/neverthrow-result/slides.md --open",
    "build": "slidev build decks/fdm-intro/slides.md",
    "build:fdm": "slidev build decks/fdm-intro/slides.md",
    "build:neverthrow": "slidev build decks/neverthrow-result/slides.md --out dist/neverthrow-result",
    "export": "slidev export decks/fdm-intro/slides.md --output dist/fdm-intro.pdf",
    "export:fdm": "slidev export decks/fdm-intro/slides.md --output dist/fdm-intro.pdf",
    "export:neverthrow": "slidev export decks/neverthrow-result/slides.md --output dist/neverthrow-result.pdf"
  },
  "dependencies": {
    "@slidev/cli": "^52.11.4",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "neverthrow": "^8.2.0",
    "typescript": "^5.7.3",
    "vue": "^3.5.27"
  }
}
